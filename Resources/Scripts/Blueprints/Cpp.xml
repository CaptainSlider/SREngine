<?xml version="1.0"?>
<Blueprints>
    <Branch Type="Blueprint">
        <Color r="100" g="100" b="100" a="255"/>
        <Inputs>
            <Pin Name="Enter" Type="Flow"/>
            <Pin Name="Condition" Type="Bool"/>
        </Inputs>
        <Outputs>
            <Pin Name="True" Type="Flow"/>
            <Pin Name="False" Type="Flow"/>
        </Outputs>
        <Code Source="
                if (Condition) {
                    $Insert(True)
                } else {
                    $Insert(False)
                }
            "/>
    </Branch>
</Blueprints>